    <link href="/css/bootstrap-theme.min.css" rel="stylesheet">
    <link href="/css/sleepData.css" rel="stylesheet">

        <script type="text/javascript">
        window.onload = function () {
          var chart = new CanvasJS.Chart("chartContainer",
          {

            title:{
              text: "Community Sleep Data"
            },
            axisX: {
              title: "Time",
            },
            axisY: {
              title: "Quality",
              interval: 25
            },
            data: [
            {
              type: "area",
              dataPoints: [
                {{#each communitydata}}
                  { x: new Date( {{year}}, {{month}}, {{day}} ), y: {{y}} },
                {{/each}}
              ]
            }
            ]

          });
          chart.render();
        }
    </script>
  </head>
  <body>
    <div class="container">
     <div id="chartContainer" style="height: 400px; width: 100%;">
    </div>

      <div class="jumbotron">
          <h1>Case Studies</h1>
        </a>
