<!DOCTYPE html>
<html>
  <head>
    <nav class="navbar navbar-inverse">
      <div class="container-fluid">
        <div class="navbar-header">
          <button type="button" class="navbar-toggle" data-toggle="collapse" data-target="#myNavbar">
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span> 
          </button>
          <a class="navbar-brand" href="/home">SleepData</a>
        </div>
        <div class="collapse navbar-collapse" id="myNavbar">
          <ul class="nav navbar-nav">
            <li> <a href="/tasks">Tasks</a> </li>
            <li> <a class ="active" href="/personal">Personal</a> </li>
            <li> <a href="/community">Community</a></li>
          </ul>
          <ul class="nav navbar-nav navbar-right">
            <li><a href="#"><span class="glyphicon glyphicon-user"></span> Sign Up</a></li>
            <li><a href="/login"><span class="glyphicon glyphicon-log-in"></span> Login</a></li>
          </ul>
        </div>
      </div>
    </nav>


    <!-- jQuery (necessary for Bootstrap's JavaScript plugins) -->
    <script src="https://code.jquery.com/jquery.js"></script>
    <script src="/js/jquery.canvasjs.min.js"></script>
    <script src="/js/bootstrap.min.js"></script>
    <script src="/js/page.js"></script>
    <script src="/js/bootstrap-clockpicker.js"></script>

    <title>Personal</title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">

    <script type="text/javascript">
        window.onload = function () {
          var chart = new CanvasJS.Chart("chartContainer",
          {

            title:{
              text: "Sleep Quality"
            },
            axisX: {
              title: "Time",
              interval: 10
            },
            axisY: {
              title: "Quality",
              interval: 25
            },
            data: [
            {
              type: "line",
              dataPoints: [
                {{#each graphdata}}
                  { x: {{x}}, y: {{y}} },
                {{/each}}
              ]
            }
            ]

          });
          chart.render();
        }
    </script>

  </head>
  <body>

  <div id="cover"></div>

	<!-- this is a comment in HTML -->
	<div class="container">
    <div id="chartContainer" style="height: 400px; width: 100%;">
  </div>


		<div class="jumbotron">
  			<h1>Alarms</h1>
        <u1 class="alarms" id = "alarms">
          {{#each alarms}}
            <li>
                <p>{{name}}: {{time}}</p>
                <label class="switch">
                  <input type="checkbox">
                  <div class="slider"></div>
                </label>
            </li>
          {{/each}}
        </u1>
        <div class="input-group clockpicker">
          <input type="time" class="form-control" value="09:30">
            <span class="input-group-addon">
              <span class="glyphicon glyphicon-time"></span>
          </span>
        </div>
        </script>
  	</div>

    <!-- Bootstrap -->
    <link href="/css/bootstrap.min.css" rel="stylesheet">
    <link href="/css/bootstrap-theme.min.css" rel="stylesheet">
    <link href="/css/bootstrap-clockpicker.css" rel="stylesheet">
    <link href="/css/sleepData.css" rel="stylesheet">
    

  </body>
</html>
