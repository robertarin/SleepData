<script>
    function gaFun() {
        var optOut = $('#optRadio input:radio:checked').val();
        console.log(optOut);
        
        if (optOut == 'true'){
          ga('send', 'event', 'opt', 'yes', {
            hitCallback: function() {
                console.log("opting out");
            }
          });
        }
        }
          
    }
</script>

